    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.34.2"
  },
  "keywords": [
    "react-calendar-carousel",
